{
  "name": "labs-server-node",
  "version": "6.3.2",
  "description": "Labs: Server (node)",
  "main": "index.ts",
  "scripts": {
    "prestart": "npm install",
    "start": "node dist/index.js",
    "build": "npm run prestart; npm run build:live",
    "clean": "rm -Rf dist",
    "build:copy": "cpx \"src/**/*.{yaml,json,html,crt,key}\" dist/",
    "build:live": "npm run clean && ./node_modules/.bin/ttsc -p tsconfig.json && npm run build:copy",
    "build:dev": "nodemon --ignore ./test --ignore ./cache --ignore ./docs --ignore ./log --exec ts-node -r tsconfig-paths/register -- ./src/index.ts",
    "dev": "NODE_EXTRA_CA_CERTS=/etc/ssl/certs/rootCA.pem npm run build:dev",
    "lint": "tslint --project ./ > log/lint.errors.txt",
    "test": "NODE_PATH=./src ./node_modules/.bin/mocha --require ts-node/register test/**/* --timeout 50000 ",
    "docs": "@powershell -Command scripts/generate_docs.ps1"
  },
  "private": true,
  "dependencies": {
    "@analytics/google-analytics": "0.5.3",
    "@analytics/google-tag-manager": "0.5.1",
    "@assistant/conversation": "^3.8.1",
    "@google-cloud/dialogflow": "^4.6.0",
    "@sendgrid/mail": "^7.7.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "analytics": "0.7.16",
    "axios": "0.21.4",
    "bluebird": "3.7.2",
    "body-parser": "1.18.3",
    "cls-hooked": "4.2.2",
    "connect-mongodb-session": "^3.1.1",
    "cors": "2.8.5",
    "cpx": "1.5.0",
    "csurf": "^1.11.0",
    "debug": "4.3.2",
    "dotenv": "16.0.3",
    "express": "4.16.3",
    "express-graphql": "0.12.0",
    "express-request-id": "1.4.1",
    "express-requests-logger": "3.0.3",
    "express-session": "^1.17.2",
    "express-useragent": "^1.0.15",
    "express-winston": "4.2.0",
    "g": "2.0.1",
    "graphql": "15.5.3",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "install": "^0.13.0",
    "js-yaml": "3.14.1",
    "mongoose": "6.0.5",
    "morgan": "1.10.0",
    "oas-tools": "^2.2.2",
    "openapi-to-graphql": "^2.6.3",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "perfy": "1.1.5",
    "rate-limiter-flexible": "^2.4.1",
    "serve-index": "1.9.1",
    "serverless-http": "^3.1.0",
    "sib-api-v3-sdk": "^8.5.0",
    "socket.io": "4.1.3",
    "ttypescript": "1.5.13"
  },
  "devDependencies": {
    "@types/cls-hooked": "4.3.3",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.13",
    "@types/express-graphql": "0.9.0",
    "@types/mongoose": "5.11.97",
    "@types/node": "16.9.1",
    "@types/node-uuid": "0.0.29",
    "@types/winston": "2.4.4",
    "chai": "4.3.4",
    "chai-openapi-response-validator": "0.13.0",
    "nodemon": "^1.19.4",
    "ts-mocha": "^8.0.0",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.4.2"
  }
}
